preprocessor :
  cd DIRECTORY
  write grid.py & model.py
  pre-sparx

solver :
  sparx run task_amc

postprocessor :
  sparx run task_lineobs	
			 lte='True' / overlap='xxkms^-1'
	    task_zeeman 
                         lte='True'
	    task_contobs
	    
	    task_coldens
        
  Interface : 
  task -- 'line'
       -- 'zeeman'
       -- 'cont'
       -- 'coldens'
       
       
line-fitting :
  (Goal) template :
        ./linefitting
         
         